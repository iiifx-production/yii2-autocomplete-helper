{
    "name": "iiifx-production/yii2-autocomplete-helper",
    "type": "extension",
    "description": "Yii2 Autocomplete Helper",
    "keywords": [
        "yii2",
        "yii",
        "framework",
        "ide",
        "autocomplete",
        "auto-completion",
        "helper"
    ],
    "homepage": "https://github.com/iiifx-production/yii2-autocomplete-helper",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Vitaliy IIIFX Khomenko",
            "email": "iiifx.me@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "yiisoft/yii2": "2.0.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "iiifx\\Yii2\\Autocomplete\\": "source/"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist",
        "preferred-update": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
