{
    "name": "iiifx-production/yii2-autocomplete-helper",
    "type": "extension",
    "description": "Yii2 Autocomplete Helper",
    "keywords": [
        "yii2",
        "yii",
        "framework",
        "ide",
        "autocomplete",
        "auto-completion",
        "helper"
    ],
    "homepage": "https://github.com/iiifx-production/yii2-autocomplete-helper",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Vitaliy IIIFX Khomenko",
            "email": "iiifx.me@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "yiisoft/yii2": "~2.0.42"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "iiifx\\Yii2\\Autocomplete\\": "source/"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "fxp-asset": {
            "npm-asset-library": "vendor/npm-asset",
            "bower-asset-library": "vendor/bower-asset"
        }
    }
}
